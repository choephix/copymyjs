{
  "name": "copymyjs",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "scripts": {
    "dev": "astro dev",
    "dev:all": "astro dev --all",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "admin": "node scripts/admin.js",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.2",
    "@astrojs/prism": "^3.3.0",
    "@astrojs/tailwind": "^6.0.2",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.14",
    "@types/node": "^22.13.10",
    "astro": "^5.12.7",
    "enquirer": "^2.4.1",
    "lucide-astro": "^0.479.0",
    "prettier": "^3.5.3",
    "shiki": "^3.2.1",
    "tailwindcss": "^4.0.14"
  }
}
