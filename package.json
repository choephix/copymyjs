{
  "name": "copymyjs",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "scripts": {
    "dev": "astro dev",
    "dev:all": "astro dev --all",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "admin": "node scripts/admin.js",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@astrojs/mdx": "^2.2.0",
    "@astrojs/prism": "^3.0.0",
    "@astrojs/tailwind": "^5.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^22.9.1",
    "astro": "^4.5.3",
    "enquirer": "^2.4.1",
    "lucide-astro": "^0.359.0",
    "prettier": "^3.3.3",
    "shiki": "^1.2.0",
    "tailwindcss": "^3.4.1"
  }
}
